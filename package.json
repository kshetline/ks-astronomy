{
  "name": "ks-astronomy",
  "version": "1.3.0",
  "description": "Astronomical calculations for planetary positions, moon phases, eclipses, rise, transit, and set times, and more.",
  "main": "dist/index.js",
  "typings": "dist/index",
  "scripts": {
    "build": "tsc && npm run resources",
    "lint": "eslint \"**/*.ts\"",
    "test": "nyc --reporter=html mocha --require ts-node/register src/**/*.spec.ts",
    "test-dev": "mocha --require ts-node/register -w --watch-extensions ts src/**/*.spec.ts",
    "prepublishOnly": "npm run build",
    "watch:build": "tsc --watch",
    "resources": "cp -R src/resources/ dist/resources/"
  },
  "keywords": [
    "asteroids",
    "astronomy",
    "calendar",
    "comets",
    "eclipses",
    "equinox",
    "grs",
    "moon",
    "planets",
    "solar",
    "solstice",
    "stars",
    "sunrise",
    "sunset"
  ],
  "author": "Kerry Shetline <kerry@shetline.com>",
  "license": "MIT",
  "dependencies": {
    "array-buffer-reader": "^1.0.4",
    "ks-date-time-zone": "^1.8.0",
    "ks-math": "^1.2.1",
    "ks-util": "^1.11.3",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.4",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "repository": "github:kshetline/ks-astronomy"
}
