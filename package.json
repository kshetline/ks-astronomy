{
  "name": "ks-astronomy",
  "version": "1.0.6",
  "description": "Astronomical calculations for planetary positions, moon phases, eclipses, rise, transit, and set times, and more.",
  "main": "dist/index.js",
  "typings": "dist/index",
  "scripts": {
    "build": "tsc && npm run resources",
    "test": "karma start",
    "prepublishOnly": "npm run build",
    "watch:build": "tsc --watch",
    "resources": "cp -R src/resources/ dist/resources/"
  },
  "keywords": [
    "asteroids",
    "astronomy",
    "calendar",
    "comets",
    "eclipses",
    "equinox",
    "grs",
    "moon",
    "planets",
    "solar",
    "solstice",
    "stars",
    "sunrise",
    "sunset"
  ],
  "author": "Kerry Shetline <kerry@shetline.com>",
  "license": "MIT",
  "dependencies": {
    "array-buffer-reader": "^1.0.3",
    "ks-date-time-zone": "^1.6.6",
    "ks-math": "^1.1.6",
    "ks-util": "^1.6.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.16",
    "@types/lodash": "^4.14.144",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-typescript": "^4.1.1",
    "karma-typescript-es6-transform": "^4.1.1",
    "tslint": "^5.20.0",
    "typescript": "^2.9.2"
  },
  "repository": "github:kshetline/ks-astronomy"
}
